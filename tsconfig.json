{
    "compilerOptions": {
      "lib": ["es2015"],
      "removeComments": true,
      "moduleResolution": "node",
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "sourceMap": true,
      "target": "es5",
      "outDir": "lib",
      "esModuleInterop": true
    },
    "include": ["./**/*.ts"],
    "exclude": [
      "node_modules/**/*",
      ".serverless/**/*",
      ".webpack/**/*",
      "_warmup/**/*",
      ".vscode/**/*"
    ]
}